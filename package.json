{
	"name": "test-code-doc",
	"version": "1.0.0",
	"description": "manual comment parser",
	"main": "index.js",
	"scripts": {
		"format": "prettier -w .",
		"generate:doc": "run-s doc:*",
		"doc:extract": "npx ts-node ./src/extractJsComment.ts",
		"doc:ToHtml": "run-s docToHtml:*",
		"predocToHtml:createHtmlStruct": "cp -r ./output/* ./html_output/input/json",
		"docToHtml:createHtmlStruct": "node ./src/createHtmlStruct.ts"
	},
	"author": "",
	"license": "ISC",
	"devDependencies": {
		"npm-run-all": "^4.1.5",
		"prettier": "^2.8.4"
	},
	"dependencies": {
		"ts-node": "^10.9.2"
	}
}